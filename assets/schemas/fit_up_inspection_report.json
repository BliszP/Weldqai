  "details": [
    { "key": "project_name", "label": "Project", "shared": true },
    { "key": "client", "label": "Client", "shared": true },
    { "key": "document_no", "label": "Document No.", "shared": true },
    { "key": "date", "label": "Date", "type": "date", "shared": true },
    { "key": "inspector", "label": "Inspector", "shared": true },
    { "key": "line_no", "label": "Line/Member No.", "type": "text" },
    { "key": "joint_id", "label": "Joint/Location ID", "type": "text" },
    { "key": "wps_no", "label": "WPS No.", "shared": true },
    { "key": "joint_type", "label": "Joint Type", "type": "choice", "choices": ["BW-Groove", "BW-V", "BW-U", "BW-J", "FW-Fillet", "Other"] },
    { "key": "position", "label": "Position", "type": "choice", "choices": ["1G", "2G", "3G", "4G", "5G", "6G", "1F", "2F", "3F", "4F"] },
    { "key": "remarks", "label": "Remarks" }
