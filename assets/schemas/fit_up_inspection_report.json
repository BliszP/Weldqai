    "ASME IX": { "details": [ { "key": "fitup_ref", "label": "Fit-up Ref", "type": "static", "value": "Per construction code (e.g., B31.3)" } ] },
    "ISO 17637": { "details": [ { "key": "quality_level", "label": "ISO 5817 Quality Level", "type": "choice", "choices": ["B", "C", "D"] } ] }
