        { "key": "requirement", "label": "Requirement", "type": "text", "width": 160 },
        { "key": "measured", "label": "Measured", "type": "text", "width": 120 },
        { "key": "status", "label": "Status", "type": "choice", "choices": ["OK", "NOK"], "width": 90 },
        { "key": "comments", "label": "Comments", "type": "text", "width": 200 }
